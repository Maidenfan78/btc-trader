{
  "name": "btc-trader",
  "version": "1.0.0",
  "private": true,
  "description": "Personal BTC/Crypto trading bot using trading-bot-platform",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:runner": "node dist/bot-runner.js",
    "start:bot": "node dist/bot-runner.js --bot-id",
    "start:dashboard": "node dist/dashboard.js",
    "start:ui": "node dist/ui-server.js",
    "dev": "ts-node src/index.ts",
    "dev:runner": "ts-node src/bot-runner.ts",
    "dev:dashboard": "ts-node src/dashboard.ts",
    "dev:ui": "ts-node src/ui-server.ts",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "test": "cross-env VITE_CJS_IGNORE_WARNING=1 vitest run",
    "test:watch": "cross-env VITE_CJS_IGNORE_WARNING=1 vitest",
    "test:ci": "cross-env VITE_CJS_IGNORE_WARNING=1 vitest run --reporter=verbose",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@jup-ag/api": "^6.0.48",
    "@solana/spl-token": "^0.1.8",
    "@solana/web3.js": "^1.98.4",
    "trading-bot-platform": "file:../trading-bot-platform"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "vitest": "^4.0.18"
  }
}
